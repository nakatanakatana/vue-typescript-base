{
  "name": "typescript-vue",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "webpack --config config/webpack.config.base.js",
    "build:storybook": "vue-cli-service build:storybook -c config/storybook",
    "clean": "rimraf dist",
    "clean:all": "npm run clean && rimraf node_modules",
    "dev": "webpack-dev-server --config config/webpack.config.base.js --hot",
    "serve:storybook": "vue-cli-service serve:storybook -p 6006 -c config/storybook"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "testMatch": [
      "**/?(*.)+(spec).ts?(x)"
    ],
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      ".*\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-class-component": "^6.2.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-typescript": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@storybook/addon-actions": "4.0.0-alpha.20",
    "@storybook/addon-links": "4.0.0-alpha.20",
    "@storybook/vue": "^4.0.0-alpha.24",
    "@types/jest": "^23.3.3",
    "@vue/cli-service": "^3.0.4",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.3",
    "ts-loader": "^5.2.1",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.1.1",
    "vue-cli-plugin-storybook": "^0.4.8",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "license": "MIT"
}
